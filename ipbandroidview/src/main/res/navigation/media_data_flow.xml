<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/media_data_flow"
    app:startDestination="@id/subInfoDialogFragmentHtml">

    <dialog
        android:id="@+id/subInfoDialogFragmentHtml"
        android:name="com.progressterra.ipbandroidview.ui.media_data.feeds_details.MediaDataDetailsHtmlFragment"
        android:label="SubInfoDialogFragmentHtml">
        <argument
            android:name="url"
            app:argType="string" />

    </dialog>
    <dialog
        android:id="@+id/subInfoDialogFragmentVideo"
        android:name="com.progressterra.ipbandroidview.ui.media_data.feeds_details.MediaDataDetailsVideoFragment"
        android:label="SubInfoDialogFragmentVideo">
        <argument
            android:name="url"
            app:argType="string" />
    </dialog>

    <fragment
        android:id="@+id/mediaDataList"
        android:name="com.progressterra.ipbandroidview.ui.media_data.MediaDataListFragment"
        android:label="MediaDataListFragment">
        <argument
            android:name="mediaDataFlowSettings"
            app:argType="com.progressterra.ipbandroidview.ui.login.settings.MediaDataSettings" />
    </fragment>
</navigation>